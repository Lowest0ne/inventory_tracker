<% @inventory.errors.full_messages.each do |message| %>
  <%= content_tag :p, message %>
<% end %>

<%= form_for @inventory do | form | %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :description %>
  <%= form.text_area :description %>
  <%= form.label :quantity %>
  <%= form.text_field :quantity %>
  <%= form.submit %>
<% end %>
